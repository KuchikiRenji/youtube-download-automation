# Redis
REDIS_URL=redis://localhost:6379

# PostgreSQL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/yt_downloads

# Server
PORT=3001
API_BASE_PATH=/api/v1

# Download
DOWNLOAD_DIR=./downloads
MAX_CONCURRENT_WORKERS=4
MAX_RETRIES_PER_TASK=3
TASK_TIMEOUT_MS=600000

# Proxy (comma-separated: http://user:pass@host:port or http://host:port)
PROXY_LIST=
PROXY_ROTATE_ON_FAILURE=true

# Scheduler
SCHEDULER_INTERVAL_MS=5000
AUTO_RETRY_FAILED_AFTER_MS=300000

# Logging
LOG_LEVEL=info
