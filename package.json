{
  "name": "youtube-download-automation",
  "version": "1.0.0",
  "description": "Scalable YouTube video downloading automation with proxy rotation",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "worker": "node src/workers/download-worker.js",
    "scheduler": "node src/scheduler/index.js",
    "api": "node src/input/api.js",
    "dev": "node --watch src/index.js",
    "migrate": "node src/storage/migrate.js",
    "setup": "node scripts/setup.js",
    "smoke-test": "node scripts/smoke-test.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "doctor": "node scripts/doctor.js"
  },
  "keywords": ["youtube", "download", "proxy", "automation", "redis", "postgresql"],
  "license": "MIT",
  "dependencies": {
    "bullmq": "^5.1.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "yt-dlp-wrap": "^2.3.10",
    "playwright": "^1.40.1",
    "dotenv": "^16.3.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  }
}
